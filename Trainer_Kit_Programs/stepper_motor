ADDR    MNEMONICS

8000    MOV DX, FF36
8003    MOV AX, 80
8006    OUT DX, AL
8007    MOV DX, FF30
800A    MOV AL, 88H
800C    OUT DX, AL
800D    CALL 8016
8010    MOV CL, 01
8012    ROR AL, CL
8014    JMP 800C
8016    MOV CX, 500H
8019    LOOP 8019
801B    RET

ROR --> Right Rotation
ROL --> Left Rotation