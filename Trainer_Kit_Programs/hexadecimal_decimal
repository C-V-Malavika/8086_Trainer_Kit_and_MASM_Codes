ADDR    MNEMONICS

8000    MOV SI,9000
8003    MOV BL,0AH 
8005    MOV AH,00H 
8007    MOV AL,[SI]
8009    CMP AL,BL 
800B    JC STORE
800D    DIV BL 
800F    INC SI
8010    MOV [SI], AH
8012    JMP TOP
8015    INC SI
8016    MOV [SI], AL
8018    HLT

9000 --> FF (Hexadecimal)

9001 --> 05
9002 --> 05
9003 --> 02